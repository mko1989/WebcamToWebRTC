{
  "name": "webcam-to-webrtc",
  "version": "1.0.0",
  "main": "server.js",
  "bin": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "pkg . --out-path=dist",
    "build-win": "pkg . --targets=node16-win-x64 --out-path=dist",
    "build-mac": "pkg . --targets=node16-macos-x64 --out-path=dist"
  },
  "pkg": {
    "assets": [
      "public/**/*"
    ],
    "targets": [
      "node16-win-x64",
      "node16-macos-x64"
    ],
    "outputPath": "dist"
  },
  "dependencies": {
    "express": "^4.18.2",
    "node-turn": "^0.0.6",
    "uuid": "^9.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  }
}